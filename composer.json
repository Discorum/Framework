{
    "name": "discorum/framework",
    "description": "The framework that powers the Discorum forum software.",
    "keywords": ["discord", "discordapp", "discorum", "forum", "framework"],
    "homepage": "https://discorum.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Nicholas Sylke",
            "email": "hello@nsylke.me"
        },
        {
            "name": "TJ Richards",
            "email": "hello@tjr.gg"
        }
    ],
    "support": {
        "issues": "https://github.com/discorum/framework/issues",
        "source": "https://github.com/discorum/framework"
    },
    "require": {
        "php": ">=7.1",
        "slim/slim": "^3.1",
        "slim/twig-view": "^2.4",
        "illuminate/database": "^5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Discorum\\": "src/Discorum/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Discorum\\Tests\\": "tests/Discorum/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
